{
  "name": "@hidoo/eslint-config",
  "version": "1.1.0",
  "description": "Sharable eslint config for my projects.",
  "packageManager": "pnpm@8.15.1",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": "8.15.1"
  },
  "main": "index.js",
  "exports": {
    ".": "./index.js",
    "./+babel": "./+babel.js",
    "./+mocha": "./+mocha.js",
    "./+node": "./+node.js",
    "./+prettier": "./+prettier.js",
    "./+stylistic": "./+stylistic.js",
    "./lib/*": "./lib/*.js"
  },
  "files": [
    "lib/",
    "+babel.js",
    "+mocha.js",
    "+node.js",
    "+prettier.js",
    "+stylistic.js",
    "index.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "format": "prettier --write .",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "test": "pnpm run --sequential '/^test:[^:]+$/'",
    "test:lint": "pnpm run --sequential '/^test:lint:[^:]+$/'",
    "test:lint:js": "eslint .",
    "test:lint:renovate": "renovate-config-validator",
    "test:unit": "cross-env NODE_ENV=test mocha test/**/*.test.js",
    "version": "pnpm run --sequential '/^(test|version:[^:]+)$/'",
    "version:changelog": "conventional-changelog -p angular -i ./CHANGELOG.md -s -r 0",
    "version:commit": "git add ./CHANGELOG.md"
  },
  "keywords": [
    "eslint-config"
  ],
  "dependencies": {
    "@babel/eslint-parser": "7.23.10",
    "@stylistic/eslint-plugin": "1.6.2",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsdoc": "48.1.0",
    "eslint-plugin-mocha": "10.3.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-sort-class-members": "1.20.0"
  },
  "devDependencies": {
    "@babel/core": "7.23.9",
    "@commitlint/cli": "18.6.1",
    "@commitlint/config-conventional": "18.6.2",
    "conventional-changelog-cli": "4.1.0",
    "cross-env": "7.0.3",
    "eslint": "8.56.0",
    "eslint-find-rules": "4.1.0",
    "husky": "9.0.10",
    "lint-staged": "15.2.2",
    "mocha": "10.3.0",
    "prettier": "3.2.5",
    "renovate": "37.183.0"
  },
  "peerDependencies": {
    "@babel/core": ">=7.2.0",
    "eslint": ">=8.0.0"
  },
  "pnpm": {
    "overrides": {
      "mem": "^8.0.0",
      "minimist": "^1.2.5"
    }
  },
  "author": "hidoo",
  "repository": {
    "type": "git",
    "url": "https://github.com/hidoo/eslint-config"
  },
  "license": "MIT"
}
