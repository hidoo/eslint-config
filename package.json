{
  "name": "@hidoo/eslint-config",
  "version": "0.6.0",
  "description": "Sharable eslint config for my projects.",
  "main": "index.js",
  "engines": {
    "node": ">=10.18.1"
  },
  "files": [
    "lib/",
    "+babel.js",
    "+mocha.js",
    "+node.js",
    "index.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "npm-run-all -s test:*",
    "test:lint": "eslint .",
    "test:unit": "cross-env NODE_ENV=test mocha test/**/*.test.js",
    "version": "npm-run-all -s test version:changelog version:commit",
    "version:changelog": "conventional-changelog -p angular -i ./CHANGELOG.md -s -r 0",
    "version:commit": "git add ./CHANGELOG.md"
  },
  "keywords": [
    "eslint-config"
  ],
  "dependencies": {
    "babel-eslint": "10.1.0",
    "eslint-plugin-eslint-comments": "3.1.2",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsdoc": "25.4.3",
    "eslint-plugin-mocha": "7.0.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-sort-class-members": "1.7.0"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "conventional-changelog-cli": "2.0.34",
    "cross-env": "7.0.2",
    "eslint": "7.0.0",
    "eslint-find-rules": "3.5.0",
    "husky": "4.2.5",
    "intelli-espower-loader": "1.0.1",
    "lint-staged": "10.2.7",
    "mocha": "7.2.0",
    "npm-run-all": "4.1.5",
    "power-assert": "1.6.1"
  },
  "peerDependencies": {
    "eslint": ">=6.0.0"
  },
  "resolutions": {
    "mem": "^6.0.0",
    "minimist": "^1.2.5"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "eslint"
    ]
  },
  "author": "hidoo",
  "repository": {
    "type": "git",
    "url": "https://github.com/hidoo/eslint-config"
  },
  "license": "MIT"
}
