{
  "name": "@hidoo/eslint-config",
  "version": "0.7.0",
  "description": "Sharable eslint config for my projects.",
  "packageManager": "yarn@1.22.21",
  "engines": {
    "node": ">=18.0.0",
    "yarn": "1.22.21"
  },
  "main": "index.js",
  "exports": {
    ".": "./index.js",
    "./+babel": "./+babel.js",
    "./+mocha": "./+mocha.js",
    "./+node": "./+node.js",
    "./+stylistic": "./+stylistic.js",
    "./lib/*": "./lib/*.js"
  },
  "files": [
    "lib/",
    "+babel.js",
    "+mocha.js",
    "+node.js",
    "+stylistic.js",
    "index.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepare": "husky",
    "test": "npm-run-all -s test:*",
    "test:lint": "eslint .",
    "test:unit": "cross-env NODE_ENV=test mocha test/**/*.test.js",
    "version": "npm-run-all -s test version:changelog version:commit",
    "version:changelog": "conventional-changelog -p angular -i ./CHANGELOG.md -s -r 0",
    "version:commit": "git add ./CHANGELOG.md"
  },
  "keywords": [
    "eslint-config"
  ],
  "dependencies": {
    "@babel/eslint-parser": "7.23.9",
    "@stylistic/eslint-plugin": "1.5.4",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsdoc": "48.0.4",
    "eslint-plugin-mocha": "10.2.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-sort-class-members": "1.19.0"
  },
  "devDependencies": {
    "@babel/core": "7.23.9",
    "@commitlint/cli": "18.6.0",
    "@commitlint/config-conventional": "18.6.0",
    "conventional-changelog-cli": "4.1.0",
    "cross-env": "7.0.3",
    "eslint": "8.56.0",
    "eslint-find-rules": "4.1.0",
    "husky": "9.0.7",
    "lint-staged": "15.2.1",
    "mocha": "10.2.0",
    "npm-run-all": "4.1.5"
  },
  "peerDependencies": {
    "@babel/core": ">=7.2.0",
    "eslint": ">=8.0.0"
  },
  "resolutions": {
    "mem": "^8.0.0",
    "minimist": "^1.2.5"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint"
    ]
  },
  "author": "hidoo",
  "repository": {
    "type": "git",
    "url": "https://github.com/hidoo/eslint-config"
  },
  "license": "MIT"
}
