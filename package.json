{
  "name": "@hidoo/eslint-config",
  "version": "1.3.1",
  "description": "Shareable config for ESlint.",
  "packageManager": "pnpm@9.14.4",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": "9.14.4"
  },
  "main": "index.js",
  "exports": {
    ".": "./index.js",
    "./+babel": "./+babel.js",
    "./+compatibility": "./+compatibility.js",
    "./+mocha": "./+mocha.js",
    "./+node": "./+node.js",
    "./+prettier": "./+prettier.js",
    "./+stylistic": "./+stylistic.js",
    "./lib/*": "./lib/*.js"
  },
  "files": [
    "lib/",
    "+babel.js",
    "+compatibility.js",
    "+mocha.js",
    "+node.js",
    "+prettier.js",
    "+stylistic.js",
    "index.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "format": "prettier --write .",
    "preinstall": "npx only-allow pnpm",
    "prepare": "git config --local core.hooksPath .githooks",
    "test": "pnpm run --sequential '/^test:[^:]+$/'",
    "test:lint": "pnpm run --sequential '/^test:lint:[^:]+$/'",
    "test:lint:js": "eslint .",
    "test:unit": "cross-env NODE_ENV=test mocha test/**/*.test.js",
    "version": "pnpm run --sequential '/^(test|version:[^:]+)$/'",
    "version:changelog": "conventional-changelog -p angular -i ./CHANGELOG.md -s -r 0",
    "version:commit": "git add ./CHANGELOG.md"
  },
  "keywords": [
    "eslint-config"
  ],
  "dependencies": {
    "@babel/eslint-parser": "7.25.9",
    "@stylistic/eslint-plugin": "2.11.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-compat": "6.0.1",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsdoc": "50.6.0",
    "eslint-plugin-mocha": "10.5.0",
    "eslint-plugin-n": "17.14.0",
    "eslint-plugin-sort-class-members": "1.21.0"
  },
  "devDependencies": {
    "@babel/core": "7.26.0",
    "@commitlint/cli": "19.4.1",
    "@commitlint/config-conventional": "19.4.1",
    "conventional-changelog-cli": "5.0.0",
    "cross-env": "7.0.3",
    "eslint": "8.57.1",
    "eslint-find-rules": "4.2.0",
    "lint-staged": "15.2.9",
    "mocha": "10.8.2",
    "prettier": "3.4.1"
  },
  "peerDependencies": {
    "@babel/core": ">=7.2.0",
    "eslint": ">=8.0.0"
  },
  "author": "hidoo",
  "repository": {
    "type": "git",
    "url": "https://github.com/hidoo/eslint-config"
  },
  "license": "MIT"
}
